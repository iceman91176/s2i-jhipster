{
	"kind": "List",
	"apiVersion": "v1",
	"metadata": {
		"name": "jhipster-s2i"
	},
	"items": [{
		"kind": "BuildConfig",
		"apiVersion": "v1",
		"metadata": {
			"name": "jhipster-s2i"
		},
		"spec": {
			"triggers": [{
				"type": "GitHub",
				"github": {
					"secret": "secret"
				}
			}, {
				"type": "Generic",
				"generic": {
					"secret": "secret"
				}
			}, {
				"type": "ImageChange",
				"imageChange": {}
			}],
			"source": {
				"type": "Git",
				"git": {
					"uri": "https://github.com/iceman91176/s2i-jhipster.git",
					"ref": "master"
				}
			},
			"strategy": {
				"type": "Docker",
				"dockerStrategy": {
					"from": {
						"kind": "DockerImage",
						"name": "docker.io/centos:centos7"
					}
				}
			},
			"output": {
				"to": {
					"kind": "ImageStreamTag",
					"name": "jhipster-s2i:latest"
				}
			},
			"resources": {}
		}
	}, {
		"kind": "ImageStream",
		"apiVersion": "v1",
		"metadata": {
			"name": "jhipster-s2i"
		},
		"spec": {
			"dockerImageRepository": "",
			"tags": [{
				"name": "latest",
				"annotations": {
					"description": "jHipster STI image",
					"iconClass": "icon-jboss",
					"tags": "builder,jhipster,java,maven,",
					"supports": "jhipster,java:8,maven:3",
					"version": "1.0"
				}
			}]
		}
	}, {
		"kind": "Template",
		"apiVersion": "v1",
		"metadata": {
			"name": "jhipster-sample-application",
			"annotations": {
				"description": "Sample jHipster Application",
				"iconClass": "icon-jboss",
				"tags": "instant-app,jHipster"
			}
		},
		"labels": {
			"application": "jhipster-sample-application",
			"createdBy": "template-jhipster-sample-application"
		},
		"parameters": [{
			"description": "Der Name der Applikation.",
			"name": "APPLICATION_NAME",
			"value": "my-jhipster",
			"displayName": "Applikations-Name",
			"required": true
		}, {
			"description": "Hostname fuer Service-Routen.Leer lassen fuer Standard-Hostname, z.b.: <application-name>.<project>.<default-domain-suffix>",
			"name": "APPLICATION_HOSTNAME",
			"displayName": "Applikations-Hostname"
		}, {
			"description": "Git-Quell URI der jHipster-Applikation",
			"name": "GIT_URI",
			"value": "https://github.com/iceman91176/jhipster-sample-app"
		}, {
			"description": "Git branch/tag reference",
			"name": "GIT_REF",
			"value": "master"
		}, {
			"description": "Git Unter-Verzeichnis ",
			"name": "CONTEXT_DIR",
			"value": ""
		}, {
			"description": "Applikationsparameter. Diese Parameter werden als Startparameter uebergeben",
			"name": "APP_OPTIONS"
		}, {
			"description": "Buildparameter. Diese Parameter koennen fuer den Build-Prozess verwendet werden",
			"name": "BUILD_OPTIONS"
		}, {
			"description": "Github trigger secret",
			"name": "GITHUB_TRIGGER_SECRET",
			"from": "[a-zA-Z0-9]{8}",
			"generate": "expression"
		}, {
			"description": "Generic build trigger secret",
			"name": "GENERIC_TRIGGER_SECRET",
			"from": "[a-zA-Z0-9]{8}",
			"generate": "expression"
		}],
		"objects": [{
			"kind": "BuildConfig",
			"apiVersion": "v1",
			"metadata": {
				"name": "${APPLICATION_NAME}"
			},
			"spec": {
				"triggers": [{
					"type": "GitHub",
					"github": {
						"secret": "${GITHUB_TRIGGER_SECRET}"
					}
				}, {
					"type": "Generic",
					"generic": {
						"secret": "${GENERIC_TRIGGER_SECRET}"
					}
				}, {
					"type": "ImageChange",
					"imageChange": {}
				}],
				"source": {
					"type": "Git",
					"git": {
						"uri": "${GIT_URI}",
						"ref": "${GIT_REF}"
					},
					"contextDir": "${CONTEXT_DIR}"
				},
				"strategy": {
					"type": "Source",
					"sourceStrategy": {
						"env": {
							"name": "BUILDER_ARGS",
							"value": "${BUILD_OPTIONS}"
						},
						"from": {
							"kind": "ImageStreamTag",
							"name": "jhipster-s2i:latest"
						}
					}
				},
				"output": {
					"to": {
						"kind": "ImageStreamTag",
						"name": "${APPLICATION_NAME}:latest"
					}
				},
				"resources": {}
			}
		}, {
			"kind": "ImageStream",
			"apiVersion": "v1",
			"metadata": {
				"name": "${APPLICATION_NAME}"
			},
			"spec": {
				"dockerImageRepository": "",
				"tags": [{
					"name": "latest"
				}]
			}
		}, {
			"kind": "DeploymentConfig",
			"apiVersion": "v1",
			"metadata": {
				"name": "${APPLICATION_NAME}"
			},
			"spec": {
				"strategy": {
					"type": "Rolling",
					"rollingParams": {
						"updatePeriodSeconds": 1,
						"intervalSeconds": 1,
						"timeoutSeconds": 600
					},
					"resources": {}
				},
				"triggers": [{
					"type": "ConfigChange"
				}, {
					"type": "ImageChange",
					"imageChangeParams": {
						"automatic": true,
						"containerNames": [
							"${APPLICATION_NAME}"
						],
						"from": {
							"kind": "ImageStreamTag",
							"name": "${APPLICATION_NAME}:latest"
						}
					}
				}],
				"replicas": 1,
				"selector": {
					"deploymentconfig": "${APPLICATION_NAME}"
				},
				"template": {
					"metadata": {
						"labels": {
							"deploymentconfig": "${APPLICATION_NAME}"
						}
					},
					"spec": {
						"containers": [{
							"name": "${APPLICATION_NAME}",
							"image": "${APPLICATION_NAME}",
							"ports": [{
								"containerPort": 8080,
								"protocol": "TCP"
							}],
							"env": {
								"name": "APP_OPTIONS",
								"value": "${APP_OPTIONS}"
							},
							"livenessProbe": {
								"tcpSocket": {
									"port": 8080
								},
								"initialDelaySeconds": 30,
								"timeoutSeconds": 1
							},
							"resources": {},
							"terminationMessagePath": "/dev/termination-log",
							"imagePullPolicy": "IfNotPresent",
							"securityContext": {
								"capabilities": {},
								"privileged": false
							}
						}],
						"restartPolicy": "Always",
						"dnsPolicy": "ClusterFirst"
					}
				}
			}
		}, {
			"kind": "Route",
			"apiVersion": "v1",
			"metadata": {
				"name": "${APPLICATION_NAME}"
			},
			"spec": {
				"host": "${APPLICATION_HOSTNAME}",
				"to": {
					"kind": "Service",
					"name": "${APPLICATION_NAME}"
				}
			}
		}, {
			"kind": "Service",
			"apiVersion": "v1",
			"metadata": {
				"name": "${APPLICATION_NAME}"
			},
			"spec": {
				"ports": [{
					"name": "${APPLICATION_NAME}-http",
					"port": 8080,
					"targetPort": 8080
				}],
				"selector": {
					"deploymentconfig": "${APPLICATION_NAME}"
				},
				"type": "ClusterIP",
				"sessionAffinity": "None"
			}
		}]
	}]
}