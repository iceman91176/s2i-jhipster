{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "jhipster-s2i"
   },
   "items": [
		{
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "jhipster-s2i"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/iceman91176/s2i-jhipster.git",
                  "ref": "master"
               }
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "centos:7"
                  }
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "jhipster-s2i:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "centos"
         },
         "spec": {
            "dockerImageRepository": "library/centos:centos7"
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "jhipster-s2i"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "jHipster STI image",
                     "iconClass": "icon-jboss",
                     "tags": "builder,jhipster,java,maven,",
                     "supports": "jhipster,java:8,maven:3",
                     "version": "1.0"
                  }
               }
            ]
         }
      }   
   ]
}